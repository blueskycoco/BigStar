!IF 0

NOTE:   This app will make boot complete event.
           
!ENDIF

WINCEOEM=1

RELEASETYPE=PLATFORM
TARGETNAME=BootCompleteApp
TARGETTYPE=PROGRAM
COPYRES=1

DEFFILE=$(TARGETNAME).def
PREPROCESSDEFFILE=0
TARGETDEFNAME=$(TARGETNAME)
#WINCETARGETFILE0=$(_RELEASELIBDIR)\$(TARGETDEFNAME).def
EXEENTRY=WinMain
TARGETLIBS=$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\coredll.lib \
			$(_PUBLICROOT)\$(_TGTPROJ)\cesysgen\oak\lib\$(_CPUINDPATH)\ossvcs.lib\

INCLUDES=.;$(_TARGETPLATROOT)\src\inc;$(_PUBLICROOT)\$(_TGTPROJ)\cesysgen\oak\inc;

CDEFINES= -DBUILDDLL -DUNICODE -DFUNCKEY
RDEFINES=$(RDEFINES) -DFUNCKEY

SOURCES = $(TARGETNAME).cpp
