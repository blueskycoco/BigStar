#  SD Memory Card client driver files

!if ("$(BSP_NOHSMMC_CH0)" == "1")&&("$(BSP_NOHSMMC_CH1)" == "1")
SKIPBUILD=1
!endif

SYNCHRONIZE_DRAIN=1
RELEASETYPE=PLATFORM

TARGETDEFNAME=SDMemory
DEFFILE=$(TARGETDEFNAME).def

TARGETNAME=SDMemory
TARGETTYPE=DYNLINK

DLLENTRY=DllEntry

!IF "$(BSP_MOVINAND)" == "1"
CDEFINES=$(CDEFINES) -D_FOR_MOVI_NAND_
!ENDIF

CDEFINES=$(CDEFINES) -D_MMC_SPEC_42_

TARGETLIBS= \
    $(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\ceddk.lib   \
    $(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\coredll.lib

SOURCELIBS=	\
    $(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\sdcardlib.lib \
    $(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\sdhclib.lib \
    $(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\sdbus.lib \

INCLUDES=..\HSMMC_inc;$(_TARGETPLATROOT)\src\inc;

SOURCES = SDMemMain.cpp   \
          SDMemDiskIO.cpp \
    	  SDMemCardIO.cpp \

