!if "$(WINCEDEBUG)" == "debug"
SKIPBUILD=1
!endif

TARGETNAME=stepldr
TARGETTYPE=PROGRAM
RELEASETYPE=PLATFORM
EXEENTRY=StartUp
NOMIPS16CODE=1

!IF "$(BSP_USEDVS)" == "1"
ADEFINES=$(ADEFINES) -pd "DVS_EN SETL {TRUE}"
!ELSE
ADEFINES=$(ADEFINES) -pd "DVS_EN SETL {FALSE}"
!ENDIF

!IF "$(SMDK6410_X5D)" == "1"
ADEFINES=$(ADEFINES) -pd "SMDK6410_X5D SETL {TRUE}"
!ELSE
ADEFINES=$(ADEFINES) -pd "SMDK6410_X5D SETL {FALSE}"
!ENDIF
ADEFINES=$(ADEFINES) -pd "BOOTLOADER SETL {TRUE}"
LDEFINES=-subsystem:native /DEBUG /DEBUGTYPE:CV /FIXED:NO
CDEFINES=$(CDEFINES) -DBOOTLOADER
TARGETLIBS =	\
		$(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\fulllibc.lib

SOURCES= \
        startup.s \
        utils.c   \
        nand_opt.s \
        nand.c    \
        main.c    

WINCETARGETFILES=stepldr_romimage
